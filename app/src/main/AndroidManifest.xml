<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.athensview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SwimActivity"
            android:label="@string/category_swim" />
        <activity
            android:name=".PartyActivity"
            android:label="@string/category_party" />
        <activity
            android:name=".SeeActivity"
            android:label="@string/category_see" />
        <activity
            android:name=".EatActivity"
            android:label="@string/category_eat"></activity>
    </application>


</manifest>